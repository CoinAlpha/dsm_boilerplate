{
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "scripts": {
    "start": "parcel index.html --open",
    "update": "node ./node_modules/github-files-fetcher --file='./updates_config/github_auth.json' --url='https://github.com/CoinAlpha/dsm/blob/master/tailwind.config.js' --out='./' && node ./node_modules/github-files-fetcher --file='./updates_config/github_auth.json' --url='https://github.com/CoinAlpha/dsm/tree/master/src/hb_ui' --out='./src' && node ./node_modules/github-files-fetcher --file='./updates_config/github_auth.json' --url='https://github.com/CoinAlpha/dsm/tree/master/specify' --out='./'",
    "build": "parcel build index.html"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.2",
    "twin.macro": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-twin": "^1.0.2",
    "github-files-fetcher": "^1.6.0",
    "parcel-bundler": "^1.12.4",
    "tailwindcss": "^3.0.13"
  }
}
